@model IEnumerable<VentaDeVehiculos.Models.Vehiculo>

@{
    ViewData["Title"] = "Index";
    Layout = null;
}

<!DOCTYPE html>
<html lang="es">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Login Simple</title>
    <!-- Enlace a Bootstrap CSS -->
    <link rel="stylesheet" href="./css/estilosCatalogo.css">
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
</head>

<body>

    <div id="nav">
        <div>
            <a src=""> Hyundai</a>
            <a src=""> Contanos</a>
            <a src=""> Servicios</a>
            <a asp-area="" asp-controller="Vehiculoes" asp-action="Index">Vehiculos</a>
            <a asp-area="" asp-controller="Login" asp-action="Index">Iniciar Sesión</a>
        </div>
        <a src=""> buscar</a>
    </div>

    <div class="Padre">

        <div id="carouselExampleCaptions" class="carousel slide" data-bs-ride="carousel">
            <div class="carousel-indicators">
                <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="0" class="active" aria-current="true" aria-label="Slide 1"></button>
                <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="1" aria-label="Slide 2"></button>
                <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="2" aria-label="Slide 3"></button>
                <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="3" aria-label="Slide 4"></button>
                <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="4" aria-label="Slide 5"></button>
                <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="5" aria-label="Slide 6"></button>
            </div>
            <div class="carousel-inner">
                <div class="carousel-item active">
                    <img src="~/fotos/11_2.webp" class="d-block w-100" alt="...">
                    <div class="carousel-caption d-none d-md-block">
                        <h5>First slide label</h5>
                        <p>Some representative placeholder content for the first slide.</p>
                    </div>
                </div>
                <div class="carousel-item">
                    <img src="~/catalogo de vehiculos/slide-202109-tucson.webp" class="d-block w-100" alt="...">
                    <div class="carousel-caption d-none d-md-block">
                        <h5>Second slide label</h5>
                        <p>Some representative placeholder content for the second slide.</p>
                    </div>
                </div>
                <div class="carousel-item">
                    <img src="~/catalogo de vehiculos/slide-202309-creta.webp" class="d-block w-100" alt="...">
                    <div class="carousel-caption d-none d-md-block">
                        <h5>Third slide label</h5>
                        <p>Some representative placeholder content for the third slide.</p>
                    </div>
                </div>
                <div class="carousel-item">
                    <img src="~/catalogo de vehiculos/slide-202109-tucson.webp" class="d-block w-100" alt="...">
                    <div class="carousel-caption d-none d-md-block">
                        <h5>Second slide label</h5>
                        <p>Some representative placeholder content for the second slide.</p>
                    </div>
                </div>
                <div class="carousel-item">
                    <img src="~/catalogo de vehiculos/slide-202309-creta.webp" class="d-block w-100" alt="...">
                    <div class="carousel-caption d-none d-md-block">
                        <h5>Second slide label</h5>
                        <p>Some representative placeholder content for the second slide.</p>
                    </div>
                </div>
                <div class="carousel-item">
                    <img src="~/catalogo de vehiculos/slide-cretagrand.webp" class="d-block w-100" alt="...">
                    <div class="carousel-caption d-none d-md-block">
                        <h5>Second slide label</h5>
                        <p>Some representative placeholder content for the second slide.</p>
                    </div>
                </div>
            </div>
            <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="visually-hidden">Previous</span>
            </button>
            <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="visually-hidden">Next</span>
            </button>
        </div>




        <div class="hija_1">
            <div class="contenedorpadre">
                <div class="imagen_1">
                    <img src="~/catalogo de vehiculos/porque-hyundai-b.jpg" width="400px" />
                </div>

                <div class="imagen_2">
                    <img src="~/catalogo de vehiculos/porque-hyundai-a.jpg" width="400px" />
                </div>

                <div class="Parrafo_1">
                    <p class="l_1"> Origen Envidiable</p>
                    <p>
                        Iniciemos por el país donde se fabrica Hyundai. Korea es un país punta de lanza en tecnología e innovación.
                    </p>
                </div>

                <div class="Parrafo_2">
                    <div clas="l1">
                        <p>
                            La marca que esta
                            reinventando la industria
                        </p>
                    </div>
                    <div class="pp">
                        <p>
                            Calidad incuestionable a un precio justo, sin cobrar caprichos, gracias a la optimización de la tecnología y la creación de empresas proveedoras propias que minimizan los gastos y que, además, por su gran calidad se han convertido en compañías líderes en sus segmentos.
                        </p>
                    </div>
                </div>

            </div>
        </div>

        <div class="Hija_2">
            <table class="table">
                <thead>
                    <tr>
                        <th>
                            @Html.DisplayNameFor(model => model.Matricula)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.Modelo)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.Stock)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.CostoVehiculo)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.Descripsion)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.UrlFoto)
                        </th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.Matricula)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Modelo)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Stock)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.CostoVehiculo)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Descripsion)
                            </td>
                            <td>
                                @if (item.UrlFoto != null)
                                {
                                    <img src="~/fotos/@item.UrlFoto" class="img-thumbnail" width="130px" />
                                }
                            </td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2024 - VentaDeVehiculos - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <!-- Enlace a Bootstrap JS y dependencias -->
    <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
</body>



</html>




